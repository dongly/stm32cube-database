<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?>
<IP DBVersion="V4.0" Name="ST_EMWIN" Version="STM32Cube_FW_F4_V1.3.0" RootFolder="Middlewares/ST/STemWin/" CodeTemplate=""
    IncludeFile="stm32f4xx.h;stm32f4xx_hal.h"
    xmlns:ns0="http://www.w3.org/2001/XMLSchema-instance"
    ns0:schemaLocation="http://mcd.rou.st.com/modules.php?name=mcu ../../../../../../../doc/V4/Development/Specifications/db/IP_Configs.xsd"
    xmlns="http://mcd.rou.st.com/modules.php?name=mcu">
    
    <!-- Audio Class -->
    <RefConfig Name="ST_EMWIN_Init" Comment="AUDIO">
        <CallLibMethod Name="USBD_Init">
            <MethodArg Name="hUsbDevice" ParameterName="hUsbDeviceHS"/>
            <!--<MethodArg Name="USBD_DESC" ParameterName="AUDIO_Desc"/>-->
            <MethodArg Name="USBD_DESC" ParameterName="HS_Desc"/>
            <MethodArg Name="id" ParameterName="DEVICE_HS"/>
        </CallLibMethod>            
        <Component Name="Middlewares:ST_EMWIN"/>
        <ConfigFile Name="USBD_CONF_AUDIO_H"/>                      
    </RefConfig>
  
    
    
    <!-- MZA here we have the description of the structure where the CallBack (function pointer) must be initialized --> 
    <LibMethod Name="USBD_Init" Comment="">
        <Argument Name="hUsbDevice" GenericType="struct" AddressOf="true"  TypeName="USBD_HandleTypeDef" Context="global;NOT_INIT">
            <Argument AddressOf="false" GenericType="simple" Name="dev_config" OptimizationCondition="equal"/>
        </Argument> 
        <Argument Comment="" AddressOf="true" Name="USBD_DESC" GenericType="simple"  TypeName="int" Context="local;NOT_INIT"/>  
        <Argument Comment="" AddressOf="false" Name="id" GenericType="simple"  TypeName="int" Context="local;NOT_INIT"/> 
    </LibMethod> 
    
    <RefComponent Cclass="Middlewares" Cgroup="ST_EMWIN" Cversion="V5.26">
        <File Name="Lib/STemWin526_CM4_Keil_ot.lib"   Category="library"/>
       
    </RefComponent>
    
    <RefConfigFile Name="USBD_CONF_AUDIO_H" Description="Header file for #define" Template="usbdconf_h.ftl">      
        <File Name="usbd_conf.h" Category="header" Version="" Condition="all"/>
        <!-- Parameters for Function and Buffer Configurations -->
        <Argument Name="USBD_MAX_NUM_INTERFACES" GenericType="simple" Comment=""/>
        <Argument Name="USBD_MAX_NUM_CONFIGURATION" GenericType="simple" Comment=""/>
        <Argument Name="USBD_MAX_STR_DESC_SIZ" GenericType="simple" Comment=""/>
        <Argument Name="USBD_SUPPORT_USER_STRING" GenericType="simple" Comment=""/>        
        <Argument Name="USBD_DEBUG_LEVEL" GenericType="simple" Comment=""/> 
        <Argument Name="USBD_LPM_ENABLED" GenericType="simple" Comment=""/>                 
        <Argument Name="USBD_SELF_POWERED" GenericType="simple" Comment=""/>
        <Argument Name="USBD_AUDIO_FREQ" GenericType="simple" Comment=""/>
    </RefConfigFile> 
    
   
</IP>
