	<RefSignal IOMode="AlternateFunctionPushPull" Name="CTS"/>
	<RefSignal IOMode="AlternateFunctionPushPull" Name="RTS"/>
	<RefSignal IOMode="AlternateFunctionPushPull" Name="DE"/>
