	</RefParameter>
	
	
	  <RefParameter Comment="Auto Baudrate Mode arg" DefaultValue="UART_ADVFEATURE_AUTOBAUDRATE_ENABLE" Name="AutoBaudRateEnable" Type="list" Group="Advanced Features" Visible="false">
